{
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"title": "Literata & Fira Sans",
	"slug": "typography-preset-7",
	"styles":{
		"typography": {
			"fontFamily": "var:preset|font-family|fira-sans",
			"fontSize": "var:preset|font-size|medium",
			"fontStyle": "normal",
			"fontWeight": "300",
			"letterSpacing": "-1",
			"lineHeight": "1.6"
		},
		"blocks":{
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|literata",
					"fontWeight": "300",
					"letterSpacing": "-2"
				}
			},
			"core/post-title": {
				"typography": {
					"fontWeight": "250",
					"letterSpacing": "-4"
				}
			}
		},
		"elements": {
			"heading":{
				"typography": {
					"fontFamily": "var:preset|font-family|literata"
				}
			}
		}
	}
}
